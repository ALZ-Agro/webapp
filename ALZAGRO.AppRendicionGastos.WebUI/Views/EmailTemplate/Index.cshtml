@model ALZAGRO.AppRendicionGastos.Application.Dtos.UserDto

<!DOCTYPE HTML PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xmlns:mso="urn:schemas-microsoft-com:office:office" xmlns:msdt="uuid:C2F41010-65B3-11d1-A29F-00AA00C14882">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>Nuevo usuario</title>
    <link href='https://fonts.googleapis.com/css?family=Raleway:400,700,300,600' rel='stylesheet' type='text/css' />
</head>
<body>
    <table width="780" style="border:1px solid #b9c0c6;font-family: 'Raleway',Arial,Helvetica,sans-serif;" align="center" cellpadding="0" cellspacing="0">
        <tr style="font-family: 'Raleway',Arial,Helvetica,sans-serif;">
            <td>
                <p align="right" style="margin-right:7.5pt;margin-bottom:7.5pt;margin-left:22.5pt;text-align:right">
                    <span style="font-size:10.0pt;font-family:'Raleway',Arial,Helvetica,sans-serif;color:#233040">
                        Administración de Usuarios
                    </span>
                </p>
                <p align="right" style="margin-right:7.5pt;margin-bottom:7.5pt;margin-left:22.5pt;text-align:right">
                    <span style="font-size:10.0pt;font-family:'Raleway',Arial,Helvetica,sans-serif;color:#233040">
                        @DateTime.Now.ToCustomLongDateString()
                    </span>
                </p>
            </td>
        </tr>
        <tr>
            <td colspan="2" valign="top" style="padding-left: 12px; padding-right:12px;padding-top: 50px;padding-bottom: 40px">
                <p class="MsoNormal">
                    <b>
                        <span style="font-size:12px;color:#233040;font-family: 'Raleway',Arial,Helvetica,sans-serif">
                            Le informamos que ya se encuentra habilitado para acceder a ALZAGRO.
                        </span>
                        <br />
                     </b><span style="font-size:12px;color:#233040;font-family: 'Raleway',Arial,Helvetica,sans-serif">
                        <br />
                        @*<a href="http://calculart.mediosdepagosrl.com.ar/" target="_blank">Haga click aquí para acceder</a>*@<br />
                        <b>Sus datos de ingreso son:</b><br />
                        <b>Usuario: @Model.Username</b><br />
                        <b>Contraseña: @Model.Password</b><u></u><u></u>
                    </span>
                </p>
            </td>
        </tr>
        <tr>
            <td colspan="2"  style="font-size:12px; font-family: 'Raleway',Arial,Helvetica,sans-serif;
                       color:#435671; padding-top:12px; padding-bottom:12px;" align="center">
            </td>
        </tr>
    </table>
    <table width="780" align="center" cellpadding="0" cellspacing="0"
           style="color:#727078; font-size:10px; font-family: 'Raleway',Arial,Helvetica,sans-serif;">
        <tr>
            <td style="text-align:justify; padding:12px">
            </td>
        </tr>
    </table>
</body>
</html>